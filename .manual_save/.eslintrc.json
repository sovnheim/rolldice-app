{
  "env": {
      "browser": true,
      "node": true
  },
  "extends": "airbnb-base",
  "globals": {},
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 5,
      "project": "tsconfig.json",
      "sourceType": "module",
      "ecmaFeatures": {
          "modules": true
      }
  },
  "plugins": [
      "@typescript-eslint",
      "@typescript-eslint/eslint-plugin-tslint"
  ],
  "settings": {},
  "rules": {
      "import/no-unresolved": "off"
  }
}